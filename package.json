    "nodemon": "^2.0.20",
    "pinia": "^2.0.23",
    "sass": "^1.55.0",
    "scss": "^0.2.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": { 
      "@types/express-fileupload": "^1.4.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/express": "^4.17.14",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.68",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
