    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.2",
